{
    "version": 2,
    "builds": [
        {
            "src": "run.py",
            "use": "@vercel/python",
            "config": {
                "maxLambdaSize": "15mb",
                "runtime": "python3.11"
            }
        },
        {
            "src": "app/static/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/static/(.*)",
            "dest": "/app/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "run.py"
        }
    ],
    "env": {
        "APP_MODULE": "app.main:app",
        "PYTHONPATH": ".",
        "PYTHON_VERSION": "3.11",
        "VERCEL": "1",
        "DATABASE_URL": "postgresql://postgres:Valar9876%40@34.87.166.243:5432/postgres",
        "DB_HOST": "34.87.166.243",
        "DB_PORT": "5432",
        "DB_NAME": "postgres",
        "DB_USER": "postgres",
        "DB_PASSWORD": "Valar9876@"
    }
} 